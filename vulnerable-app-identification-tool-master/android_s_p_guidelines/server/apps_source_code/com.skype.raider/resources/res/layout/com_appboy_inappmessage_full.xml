<?xml version="1.0" encoding="utf-8"?>
<com.appboy.ui.inappmessage.views.AppboyInAppMessageFullView xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/com_appboy_inappmessage_full_frame" style="@style/Appboy_InAppMessage_Frame_Full"/>
    <RelativeLayout android:id="@+id/com_appboy_inappmessage_full" style="@style/Appboy_InAppMessage_Full">
        <LinearLayout android:orientation="vertical" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent" android:weightSum="1">
            <ViewStub android:id="@+id/com_appboy_inappmessage_full_imageview_stub" android:layout="@layout/com_appboy_stubbed_inappmessage_image_view" style="@style/Appboy_InAppMessage_Image_Full"/>
            <ViewStub android:id="@+id/com_appboy_inappmessage_full_drawee_stub" android:layout="@layout/com_appboy_stubbed_inappmessage_drawee_view" style="@style/Appboy_InAppMessage_Image_Full"/>
            <RelativeLayout android:id="@+id/com_appboy_inappmessage_full_textarea" style="@style/Appboy_InAppMessage_Full_TextArea">
