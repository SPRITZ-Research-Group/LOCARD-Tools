        <RelativeLayout android:padding="10dp" android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageButton android:id="@+id/com_appboy_inappmessage_full_close_button" style="@style/Appboy_InAppMessage_CloseButton_Full"/>
