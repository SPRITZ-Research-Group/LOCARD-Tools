<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/com_appboy_feedback_root" style="@style/Appboy_Feedback_Layout">
    <LinearLayout android:id="@+id/com_appboy_feedback_navigation_bar" style="@style/Appboy_Feedback_NavigationBar">
        <Button android:id="@+id/com_appboy_feedback_cancel" style="@style/Appboy_Feedback_NavigationBarCancelButton"/>
        <Button android:id="@+id/com_appboy_feedback_send" style="@style/Appboy_Feedback_NavigationBarSendButton"/>
    </LinearLayout>
    <ScrollView android:id="@+id/com_appboy_feedback_scroll" style="@style/Appboy_Feedback_Scroll">
        <LinearLayout android:id="@+id/com_appboy_feedback_body" style="@style/Appboy_Feedback_Body">
            <EditText android:id="@+id/com_appboy_feedback_message" style="@style/Appboy_Feedback_MessageEditText"/>
            <EditText android:id="@+id/com_appboy_feedback_email" style="@style/Appboy_Feedback_EmailEditText"/>
            <CheckBox android:id="@+id/com_appboy_feedback_is_bug" style="@style/Appboy_Feedback_IsBugCheckBox"/>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
